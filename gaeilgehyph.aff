# headers:options
nroffchars	().\\*
texchars	()\[]{}<\>\\$*.%
allaffixes on                         
compoundwords off

# headers:char-sets
defstringtype "nroff" "nroff" ".mm" ".ms" ".me" ".man" ".NeXT"

boundarychars	|
boundarychars	'
